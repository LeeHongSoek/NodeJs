                    </jqx-tree-items>
                </jqx-tree-item>
                <jqx-tree-item>
                    Support
                    <jqx-tree-items>
                        <jqx-tree-item>Support home</jqx-tree-item>
                        <jqx-tree-item>Customer Service</jqx-tree-item>
                        <jqx-tree-item>Knowledge base</jqx-tree-item>
                        <jqx-tree-item>Books</jqx-tree-item>
